{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowIAMUserFullAccess",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::YOUR_ACCOUNT_ID:user/YOUR_IAM_USER_NAME"
      },
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:ListBucket",
        "s3:GetObjectVersion"
      ],
      "Resource": [
        "arn:aws:s3:::ibyet-investing",
        "arn:aws:s3:::ibyet-investing/*"
      ]
    },
    {
      "Sid": "PublicReadThumbnails",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::ibyet-investing/ibyet-investing-folder/courses/*/v*/thumbnails/*"
    }
  ]
}

